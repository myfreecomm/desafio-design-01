{
  "name": "dev-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for developing static sites using Webpack & LESS",
  "author": "Victor Soares",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open --hot --mode development",
    "build": "rm -rf ./public && webpack -p --env production --mode production"
  },
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "ie 10"
  ],
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^2.0.1",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "normalize.css": "^8.0.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
